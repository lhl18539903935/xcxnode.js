/* test/a/a.wxss */
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
    font-family:"iconfont" !important;
    font-size:16px;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}
.icon-guojiwuliu:before { content: "\e7ae"; }
.icon-guojiwuliu{
    position: relative;
    top: 6px;
    font-size: 22px;
    color: #77f;
}
.icon-tequanzhuanshu:before { content: "\e7b9"; }



.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
    bottom: 6px;
    left:85%;
}
.swiper-box .wx-swiper-dot{
    width:12rpx;
    height: 12rpx;
}
.imgs{
    width: 100%;
    height: 100%;
}
.aa2{
    background-color: #fff;
    margin-bottom: 10px;
    padding-bottom: 5px;
}
.aa-view{
    display: inline-block;
    width: 20vw;
    text-align: center;
}
.aa-title{
    font-size: 12px;
}
.shop{
    margin-top: 10px;
    width: 100%;
    padding-bottom: 10px;
}
.shop-img{
    width: 100vw;
    height: 50vw;
}
.shop-p{
    margin: 10px;
    font-size: 14px;
}
.shop-qian{
    overflow: hidden;
}
.shop-5{
    color: red;
    font-weight:600;
    font-size: 18px;
    line-height: 30px;
}
.shop-6{
    font-size: 12px;
    color: #777;
    line-height: 30px;
}
.shop-7{
    float: right;
    padding: 3px 10px;
    background-color: #f55;
    color: #fff;
    margin-right: 15px;
    margin-top: 5px;
    font-size: 12px;
}
.ul{
    margin-top: 10px;
    width: 100%;
    overflow: hidden;
}
.li{
    float: left;
    width: 50%;
    box-sizing: border-box;
    border: 1px solid #f8f8f8;
    font-size: 12px;
}
.ul-img{
    width: 100%;
    height: calc(50vw - 2px);

}
.li-p{
    margin: 8px 5px;
}
.li-title{
    padding: 2px 5px;

}
.li-right{
    float: right;
    color: #777;
    font-size: 10px;
    margin-right: 5px;
}